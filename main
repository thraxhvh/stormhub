    cc = c
    while cc do task.wait()
        plr.Character.Humanoid:EquipTool(plr.Backpack:FindFirstChild("Hatchet"))
        tp(getTree().WoodHitPart.CFrame * CFrame.new(0,-3,0))
        hit()
    end
end})

